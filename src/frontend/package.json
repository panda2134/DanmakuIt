{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint": "yarn lint:js",
    "test": "yarn test:unit && yarn test:e2e",
    "test:unit": "cross-env TEST=unit ava ./test/specs/**/*",
	  "pretest:e2e": "nuxt build && nuxt generate",
    "test:e2e": "cross-env TEST=e2e playwright test ./test/e2e/**/*",
    "openapi": "yarn openapi-typescript https://danmakuit.panda2134.site/api/v1/openapi.json --output openapi/openapi.ts"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "core-js": "^3.20.1",
    "fast-copy": "^2.1.1",
    "nanoid": "^3.1.30",
    "nuxt": "^2.15.7",
    "openapi-typescript": "^4.4.0",
    "portal-vue": "^2.1.7",
    "reconnecting-websocket": "^4.4.0",
    "throttle-debounce": "^3.0.1",
    "vee-validate": "^3.4.14",
    "vuetify": "^2.5.5",
    "vuetify-toast-snackbar": "^0.6.1"
  },
  "devDependencies": {
    "@ava/babel": "^2.0.0",
    "@babel/eslint-parser": "^7.16.5",
    "@iconify/vue2": "^1.1.1",
    "@mdi/font": "^6.5.95",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^8.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/vuetify": "^1.12.3",
    "@playwright/test": "^1.17.1",
    "@types/throttle-debounce": "^2.1.0",
    "@vue/test-utils": "^1.2.1",
    "ava": "^3.15.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.33.0",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-vue": "^8.2.0",
    "jsdom": "^19.0.0",
    "jsdom-global": "^3.0.2",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^1.0.0",
    "require-extension-hooks-vue": "^3.0.0"
  }
